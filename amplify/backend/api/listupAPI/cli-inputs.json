{
  "version": 1,
  "paths": {
    "/user/{email}": {
      "name": "/user/{email}",
      "lambdaFunction": "listupGetUser",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/categories/{category}": {
      "name": "/categories/{category}",
      "lambdaFunction": "listupGetCategory",
      "permissions": {
        "setting": "open"
      }
    },
    "/user/{email}/lists": {
      "name": "/user/{email}/lists",
      "lambdaFunction": "listupGetList",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/user/{email}/lists/{list}": {
      "name": "/user/{email}/lists/{list}",
      "lambdaFunction": "listupGetSingleList",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    }
  }
}